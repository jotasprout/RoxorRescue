  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "prismjs": "^1.6.0",
    "stackblur-canvas": "^1.4.0",
    "twilio": "^3.0.0-rc.16",
    "twilio-video": "^1.3.0"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "concurrently": "^3.5.1",
